<nav class="breadcrumbs" property="breadcrumb">
	<div class="container">
		<div class="row">
			<ol>
				<li><a href="[% c.uri_for(c.controller("Allele").action_for("form")) %]">[% c.loc("home.page.name") | html %]</a></li>
				<li><a href="[% c.uri_for(c.controller("Allele").action_for("batch_allele_query")) %]">[% c.loc("shared.batch.allele.query.text") | html %]</a></li>
				<li><a class="disabled" tabindex="-1" href="#">[% c.loc("shared.batch.allele.query.example.text") | html %]</a></li>
			</ol>
		</div>
	</div>
</nav>

<main role="main" property="mainContentOfPage" class="container fade in">

	<h1 id="wb-cont">[% c.loc("shared.batch.allele.query.example.text") | html %]</h1>

	<br>

	<div style="word-wrap:break-word;">

	<br>

		<div class="panel panel-info" style="border:none !important;">

			<div class="panel-heading" style="background-color:#dce1e7; color:#000000; border:none !important;">
				<br>
				<p class="details_labels">[% c.loc("batch.allele.query.example.instructions.msg") | html %]</p>
			</div>

			  <div class="panel-body" id="example-text" style="font-size:18px; margin-top:10px;">

					>penA_1 <span class="glyphicon glyphicon-info-sign"
					rel="popover-dt"
					tabindex="0"
					data-placement="right"
					data-original-title=""
					data-html="true"
					data-content="penA - locus name<br> 1 - sample number"></span> 
					<br>
					>penA_2 <span class="glyphicon glyphicon-info-sign"
					rel="popover-dt"
					tabindex="0"
					data-placement="right"
					data-original-title=""
					data-html="true"
					data-content="penA - locus name<br> 2 - sample number"></span> 
					<br>
					>mtrR_1 <span class="glyphicon glyphicon-info-sign"
					rel="popover-dt"
					tabindex="0"
					data-placement="right"
					data-original-title=""
					data-html="true"
					data-content="mtrR - locus name<br> 1 - sample number"></span> 
					<br>
					...
					<br>
					...
			  </div>

		</div>

	</div>


	<button type="button" class="btn btn-primary bkbtn" onClick="history.go(-1);return true;">
		<span class="glyphicon glyphicon-arrow-left"></span> [% c.loc("shared.back.btn.text") | html %]
	</button>

</main>
